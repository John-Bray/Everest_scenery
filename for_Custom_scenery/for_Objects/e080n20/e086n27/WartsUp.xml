<?xml version="1.0"?>

<PropertyList>

<path>WartsUp.ac</path>

<particlesystem>
		<name>steam</name>
		<texture>smoke.png</texture>
		<emissive>false</emissive>
		<lighting>false</lighting>
		<offsets><x-m>6.97</x-m><y-m>0.05</y-m><z-m>1</z-m></offsets>
		<attach>world</attach>
		<placer><type>point</type></placer>

		<shooter>
			<theta-min-deg>87.5</theta-min-deg>
			<theta-max-deg>92.5</theta-max-deg>
			<phi-min-deg>-2.5</phi-min-deg>
			<phi-max-deg>2.5</phi-max-deg>
			<speed-mps>
				<value>1.0</value>
				<spread>0.25</spread>
			</speed-mps>
			<rotation-speed>
				<x-max-deg-sec>15</x-max-deg-sec>
				<x-min-deg-sec>-15</x-min-deg-sec>
				<y-max-deg-sec>15</y-max-deg-sec>
				<y-min-deg-sec>-15</y-min-deg-sec>
				<z-max-deg-sec>15</z-max-deg-sec>
				<z-min-deg-sec>-15</z-min-deg-sec>
			</rotation-speed>
		</shooter>

<counter><particles-per-sec><value>20</value><spread>5</spread></particles-per-sec></counter>

<align>billboard</align>

<particle>
  <start><color><red><value>0.95</value></red><green><value>0.95</value></green><blue><value>0.95</value></blue><alpha><value>0.05</value></alpha></color><size><value>3.0</value></size></start>
  <end>  <color><red><value>1.00</value></red><green><value>1.00</value></green><blue><value>1.00</value></blue><alpha><value>0.50</value></alpha></color><size><value>6.0</value></size></end>
  <life-sec><value>1.0</value></life-sec>
  <mass-kg>0.0025</mass-kg>
  <radius-m>0.1</radius-m>
</particle>

<program><fluid>air</fluid><gravity>true</gravity><wind>true</wind></program>

</particlesystem>
<particlesystem>
		<name>smokeFire</name>
		<texture>smoke.png</texture>
		<emissive>false</emissive>
		<lighting>false</lighting>
		<offsets><x-m>-5</x-m><y-m>7</y-m><z-m>8</z-m></offsets>
		<attach>world</attach>
		<placer><type>point</type></placer>

		<shooter>
			<theta-min-deg>87.5</theta-min-deg>
			<theta-max-deg>92.5</theta-max-deg>
			<phi-min-deg>-2.5</phi-min-deg>
			<phi-max-deg>2.5</phi-max-deg>
			<speed-mps>
				<value>0.5</value>
				<spread>0.1</spread>
			</speed-mps>
			<rotation-speed>
				<x-max-deg-sec>15</x-max-deg-sec>
				<x-min-deg-sec>-15</x-min-deg-sec>
				<y-max-deg-sec>15</y-max-deg-sec>
				<y-min-deg-sec>-15</y-min-deg-sec>
				<z-max-deg-sec>15</z-max-deg-sec>
				<z-min-deg-sec>-15</z-min-deg-sec>
			</rotation-speed>
		</shooter>

<counter><particles-per-sec><value>8</value><spread>4</spread></particles-per-sec></counter>

<align>billboard</align>

<particle>
  <start><color><red><value>0.95</value></red><green><value>0.60</value></green><blue><value>0.60</value></blue><alpha><value>0.05</value></alpha></color><size><value>0.4</value></size></start>
  <end>  <color><red><value>1.00</value></red><green><value>1.00</value></green><blue><value>1.00</value></blue><alpha><value>0.50</value></alpha></color><size><value>2.4</value></size></end>
  <life-sec><value>3.0</value></life-sec>
  <mass-kg>0.0005</mass-kg>
  <radius-m>0.10</radius-m>
</particle>

<program><fluid>air</fluid><gravity>true</gravity><wind>true</wind></program>

</particlesystem>


</PropertyList>